<div id="bibliodata" class="form-horizontal">
    <div id="pub-type-div" class="form-group">
        <label class="control-label col-sm-3" for="pub-type-input">Publication Type</label>
        <div class="col-sm-9">
            <select id="pub-type-input" class="form-control" data-placeholder="Select a publication type">
            </select>
        </div>
    </div>
    <div id="pub-subtype-div" class="form-group">
        <label class="control-label col-sm-3" for="pub-subtype-input">Publication Subtype</label>
        <div class="col-sm-9">
            <select id="pub-subtype-input" class="form-control" data-placeholder="Select a publication subtype">
            </select>
        </div>
    </div>
    <div id="series-title-tiv"  class="form-group">
        <label class="control-label col-sm-3" for="series-title-input">Series Title</label>
		<div class="col-sm-9">
			<select id="series-title-input" class="form-control" data-placeholder="Select a series">
			</select>
        </div>
    </div>
    <div class="form-group" id="cost-centers-div">
        <label class="control-label col-sm-3" for="cost-centers-input">Cost Centers</label>
        <div class="col-sm-9">
            <select id="cost-centers-input" class="form-control" data-placeholder="Select one or more cost centers" multiple>
            </select>
        </div>
    </div>
    <div class="form-group" id="subseries-div">
